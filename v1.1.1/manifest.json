{
  "manifest_version": 2,

  "name": "WiKey Finder",
  "description": "Cette extension permet d'effectuer une recherche de mot-clef sur un site Wiki.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icone.png",
    "default_popup": "popup.html",
    "default_title": "Click here!"
  },
  "permissions": [
	"tabs",
    "activeTab",
	"<all_urls>"
  ],
  "content_scripts": [
	{
	  "matches": ["<all_urls>"],
	  "js": ["core.js",  "algorithmTextRank.js",  "algorithmTopicRank.js","lexicon.js_", "POSTagger.js", "graph_TextRank.js", "lexer.js", "preTraitement.js", "stemmer/lib/Snowball.min.js" ]
	}
  ]
}